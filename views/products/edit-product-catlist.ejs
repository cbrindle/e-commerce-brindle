<!DOCTYPE html>
<html lang="en">

<% include ../partials/head %>

<body>
    <% include ../partials/nav %>
    <br><br>

    <% for (let s of success) { %>
        <div class="alert alert-primary alert-dismissible fade show" role="alert">
        
            <%= s %>
        
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
    <% } %>

    <h1 class="text-center">Select a Category</h1>

    <% for (category of data) { %>
    <div class="list-group mx-auto" style="width: 40%;">
        <a href="/api/admin/editproduct/<%= category._id %>" class="list-group-item list-group-item-action"><%= category.name %></a>
    </div>
    
    
    <% } %>
    


    <% include ../partials/footer %>
</body>
</html>